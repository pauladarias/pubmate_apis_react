Form.Label>Group</Form.Label><br></br>\n        <Form.Control\n          className=\"addpub__input\"\n          type=\"group\"\n          value={fields.group}\n          onChange={handleFieldChange}\n        /><br></br>\n      </Form.Group>\n      <Button\n        className=\"addpub__save addpub__buttons\"\n        block\n        size=\"lg\"\n        type=\"submit\"\n        //disabled={!validateForm()}\n      >\n        SAVE\n      </Button>\n      <Link className=\"addpub__cancel addpub__buttons\" to=\"/\">CANCEL</Link>\n    </Form>  \n  </div>\n )\n}\n\nexport default AddPub","/Users/paulandariasperez/Desktop/PUBmate/DEV/pubmate_apis_react/src/components/SideBar.js",["69"],"import React from 'react'\nimport \"./SideBar.css\"\nimport Logo from \"./logo.png\"\n\n function SideBar() {\n  return (\n    <div className=\"sidebar\">\n      <img src={Logo} href=\"/\"></img>\n    </div>\n  )\n}\n\nexport default SideBar\n","/Users/paulandariasperez/Desktop/PUBmate/DEV/pubmate_apis_react/src/components/Heading.js",[],["70","71"],"/Users/paulandariasperez/Desktop/PUBmate/DEV/pubmate_apis_react/src/components/PubList.js",["72","73"],"import React, { useState, useEffect } from 'react'\nimport \"./PubList.css\"\nimport { Link } from \"react-router-dom\"\nimport axios from \"../axios\"\nimport { Table, TableContainer, TableHead, TableCell, TableBody, TableRow } from \"@material-ui/core\"\nimport { Edit, Delete } from \"@material-ui/icons\"\n\nfunction PubList() {\n  const [pubs, setPubs] = useState([])\n\n  const getPubs=async()=> {\n    await axios.get(\"pubmate/api/0.1/pub/2/all\")\n    .then(response=>{\n      console.log(response)\n      setPubs(response.data.pub)\n    })\n\n  }\n\n  useEffect(async()=> {\n    await getPubs()\n  }, [])\n\n\n  // useEffect(()=> {\n  //   async function fetchData() {\n  //     const request = await axios.get(\"pubmate/api/0.1/pub/2/all\")\n  //     console.log(request)\n  //     setPubs(request.data.pub)\n  //     return request\n  //   }\n  